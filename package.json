{
  "name": "headless-wp-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "deploy": "now --npm --public",
    "alias": "now alias"
  },
  "now": {
    "alias": [
      "headlesswp",
      "headlesswp.app"
    ]
  },
  "dependencies": {
    "babel-plugin-emotion": "^9.2.6",
    "babel-plugin-styled-components": "^1.5.1",
    "express": "^4.14.0",
    "isomorphic-unfetch": "^2.1.1",
    "next": "latest",
    "react": "^16.0.0",
    "react-burger-menu": "^2.5.2",
    "react-dom": "^16.0.0",
    "styled-components": "^3.4.2"
  },
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 2 versions",
            "safari >= 7"
          ]
        }
      }
    ]
  ]
}
